{
    "name": "akaunting/laravel-sortable",
    "description": "Sortable behavior package for Laravel",
    "keywords": [
        "laravel",
        "sort",
        "sortable",
        "sorting",
        "model",
        "view",
        "blade",
        "column"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Denis DuliÃ§i",
            "email": "info@akaunting.com",
            "homepage": "https://akaunting.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "laravel/framework": ">=6.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.0",
        "orchestra/testbench": ">=6.0"
    },
    "autoload": {
        "psr-4": {
            "Akaunting\\Sortable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akaunting\\Sortable\\Tests\\": "tests"
        }
    },
    "extra": {
